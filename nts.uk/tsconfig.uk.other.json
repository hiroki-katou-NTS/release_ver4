{
    "compilerOptions": {
		"target": "ES5",
        "module": "none",
		"moduleResolution": "Classic",
        "noImplicitAny": true,
        "removeComments": false,
        "preserveConstEnums": true,
		"charset": "utf8",
		"skipLibCheck": true,
        "sourceMap": true
    },
	 "include": [
        "uk.at/**/*",
		"uk.com/**/*"
    ]
}