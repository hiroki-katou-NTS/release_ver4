package nts.uk.ctx.basic.infra.entity.organization.position.laitv;

import javax.persistence.Basic;
import javax.persistence.Column;

import lombok.AllArgsConstructor;
import lombok.NoArgsConstructor;

/**
 * @author laitv
 *
 */


@AllArgsConstructor
@NoArgsConstructor
public class CmnmtJobTitle_PK_ba {
	
	@Basic(optional = false)
	@Column(name = "CCD")
	String companyCode;
	
	@Basic(optional = false)
	@Column(name = "JOBCD")
	String jobCode;
	
	@Basic(optional = false)
	@Column(name ="HIST_ID")
	public String histId;
}
