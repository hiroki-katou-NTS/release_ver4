package nts.uk.ctx.basic.app.find.position.laitv;

import java.util.Date;

import lombok.Getter;
import lombok.Setter;
import lombok.Value;
import nts.uk.ctx.basic.organization.position_ba.Position_ba;

/**
 * @author laitv
 *
 */
@Getter
@Setter
@Value
public class PositionDto_ba {
	Date endDate;

	Date startDate;

	String companyCode;

	String jobCode;

	String jobName;

	String meMo;

	String jobOutCode;

	public static PositionDto_ba fromDomain(Position_ba domain){
		 return new PositionDto_ba(domain.getEndDate().date(), domain.getStartDate().date(), domain.getCompanyCode(), domain.getJobCode(), domain.getJobName(), domain.getMemo(), domain.getJobOutCode())
	 }
}
